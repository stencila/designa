{
  "name": "@stencila/components",
  "version": "0.47.4",
  "description": "Stencila Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencila-components/stencila-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --dev --watch --docs",
    "build:docs": "custom-elements-manifest analyze --stencil --globs 'src/**/*.tsx' --outdir dist",
    "kill": "pkill -f \"Stencil: stencila-components\"",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write './src/**/*.{css,js,json,md,ts,yaml}'",
    "prepublishOnly": "node ./scripts/prepare.mjs && npm run build"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.5.7",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@stencil/core": "2.12.0",
    "@stencil/postcss": "2.1.0",
    "@stencila/eslint-config-stencil": "2.0.4",
    "@stencila/schema": "1.12.0",
    "@types/d3": "7.1.0",
    "@types/d3-drag": "3.0.1",
    "@types/d3-force": "3.0.3",
    "@types/d3-hierarchy": "3.0.2",
    "@types/d3-polygon": "3.0.0",
    "@types/d3-selection": "3.0.2",
    "@types/d3-shape": "3.0.2",
    "@types/d3-zoom": "3.0.1",
    "@types/feather-icons": "4.7.0",
    "@types/jest": "27.0.3",
    "@types/plotly.js": "1.54.18",
    "@types/pluralize": "0.0.29",
    "@types/puppeteer": "5.4.4",
    "@types/webfontloader": "1.6.34",
    "chokidar-cli": "3.0.0",
    "fp-ts": "2.11.5",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "postcss-custom-properties": "12.0.1",
    "postcss-import": "14.0.2",
    "puppeteer": "13.0.1",
    "tailwindcss": "2.2.19"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.19.3",
    "@codemirror/closebrackets": "^0.19.0",
    "@codemirror/commands": "^0.19.4",
    "@codemirror/comment": "^0.19.0",
    "@codemirror/fold": "^0.19.0",
    "@codemirror/gutter": "^0.19.2",
    "@codemirror/highlight": "^0.19.4",
    "@codemirror/history": "^0.19.0",
    "@codemirror/lang-javascript": "^0.19.0",
    "@codemirror/lang-json": "^0.19.0",
    "@codemirror/lang-markdown": "^0.19.0",
    "@codemirror/lang-python": "^0.19.0",
    "@codemirror/lang-xml": "^0.19.0",
    "@codemirror/legacy-modes": "^0.19.0",
    "@codemirror/panel": "^0.19.0",
    "@codemirror/search": "^0.19.1",
    "@codemirror/state": "^0.19.2",
    "@codemirror/stream-parser": "^0.19.2",
    "@codemirror/view": "^0.19.7",
    "@nll/datum": "^3.5.0",
    "@popperjs/core": "^2.11.0",
    "@stencila/executa": "^1.15.7",
    "@stencila/style-material": "0.16.1",
    "@stencila/style-stencila": "0.27.1",
    "animate-presence": "^0.2.1",
    "d3": "^7.0.1",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-hierarchy": "^3.0.0",
    "d3-polygon": "^3.0.1",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "feather-icons": "^4.28.0",
    "mem": "8.1.1",
    "pluralize": "^8.0.0",
    "vega": "^5.20.2",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.0",
    "webfontloader": "^1.6.28",
    "wretch": "^1.7.5"
  },
  "peerDependencies": {
    "@stencila/schema": "^1.9.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "."
    },
    "extends": "@stencila/eslint-config-stencil",
    "rules": {
      "array-callback-return": "off",
      "@stencil/element-type": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "jsx-a11y/no-static-element-interactions": "warn",
      "jsx-a11y/click-events-have-key-events": "warn",
      "jsx-a11y/no-onchange": "off"
    }
  },
  "customElements": "dist/custom-elements.json"
}

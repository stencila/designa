{
  "name": "@stencila/brand",
  "version": "0.6.6",
  "description": "Stencila brand assets",
  "keywords": [
    "Stencila",
    "design",
    "brand",
    "identity"
  ],
  "author": "Stencila <hello@stenci.la>",
  "homepage": "https://stenci.la",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/style.git"
  },
  "scripts": {
    "build": "parcel build ./src/* ./src/**/* ./src/**/**/* -d ./dist --public-url ./",
    "build:watch:brand": "parcel watch ./src/* ./src/**/* ./src/**/**/* -d ./dist --public-url ./",
    "moveFonts": "mv ./dist/*.woff* ./dist/fonts"
  },
  "bugs": {
    "url": "https://github.com/stencila/style/issues"
  },
  "devDependencies": {
    "@abide-community/parcel-plugin-clean-out-dir": "1.0.0",
    "concurrently": "5.3.0",
    "parcel-plugin-css-pretransform": "1.0.0"
  }
}
